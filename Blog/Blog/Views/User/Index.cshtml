@model IEnumerable<Blog.Entities.Article>

<div>
    <h3>Блог</h3>
    <br />
</div>

<table class="distance">

    @foreach (var a in Model)
    {
        <tr>
            <td><p>@a.Title</p></td>
            <td><p>@a.CreationDate</p></td>
            <td><p><a href="/User/Read/@a.Id">Читать</a></p></td>
        </tr>
    }
</table>